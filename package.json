{
  "name": "react-redux-universal-hot-example",
  "description": "Example of an isomorphic (universal) webapp using react redux and hot reloading",
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/react-redux-universal-hot-example"
  },
  "homepage": "https://github.com/erikras/react-redux-universal-hot-example",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "precommit": "lint-staged",
    "precommit-prettier": "eslint --print-config .eslintrc.js | prettier-eslint --write",
    "start": "npm run start-prod & npm run start-prod-api",
    "start-prod": "NODE_PATH=./src NODE_ENV=production PORT=8080 APIPORT=3030 node ./bin/server.js",
    "start-prod-api": "NODE_PATH=./api NODE_ENV=production APIPORT=3030 node ./bin/api.js",
    "dev": "npm run watch-client & npm run start-dev & npm run start-dev-api",
    "start-dev": "NODE_PATH=./src NODE_ENV=development PORT=3000 APIPORT=3030 WEBPACK_DLLS=1 node ./bin/server.js",
    "start-dev-api": "NODE_PATH=./api NODE_ENV=development APIPORT=3030 node ./bin/api.js",
    "watch-client": "NODE_PATH=./src NODE_ENV=development PORT=3000 APIPORT=3030 WEBPACK_DLLS=1 node webpack/webpack-dev-server.js",
    "build": "NODE_ENV=production webpack --colors --display-error-details --config=webpack/prod.config.js",
    "build-dlls": "webpack --colors --display-error-details --config webpack/vendor.config.js",
    "postinstall": "npm run build & npm run build-dlls",
    "prettier": "npm run precommit-prettier -- \"./+(src|api)/**/*.js\"",
    "lint": "eslint -c .eslintrc api src",
    "test": "karma start",
    "test-node": "jest $(find api -name '*-test.js')",
    "test-node-watch": "jest $(find api -name '*-test.js') --watch",
    "docker-build": ". ./docker/build.sh",
    "docker-dev": ". ./docker/run.dev.sh",
    "docker-prod": ". ./docker/run.prod.sh",
    "lighthouse-report": "LIGHTHOUSE_CHROMIUM_PATH=$(which google-chrome) lighthouse http://localhost:8080"
  },
  "lint-staged": {
    "+(src|api)/**/*.js": [
      "npm run precommit-prettier",
      "eslint -c .eslintrc",
      "git add"
    ]
  },
  "dependencies": {
    "async": "^3.2.4",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "ethereum-blockies": "^0.1.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-proxy": "^1.18.1",
    "is-promise": "^4.0.0",
    "js-cookie": "^3.0.1",
    "localforage": "^1.10.0",
    "lru-memoize": "^1.1.0",
    "morgan": "^1.10.0",
    "multireducer": "^3.1.1",
    "nedb": "^1.8.0",
    "passport-facebook-token": "^4.0.0",
    "pretty-error": "^4.0.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^17.0.2",
    "react-fontawesome": "^1.7.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.9",
    "react-router": "^5.3.4",
    "react-router-bootstrap": "^0.26.2",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.4",
    "redux": "^4.2.1",
    "redux-auth-wrapper": "^2.0.3",
    "redux-connect": "^5.0.0",
    "redux-form": "^8.3.9",
    "redux-persist": "^6.0.0",
    "serialize-javascript": "^6.0.1",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "superagent": "^8.1.2",
    "verror": "^1.10.1",
    "web3": "^4.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.6.2",
    "babel-loader": "^8.3.0",
    "bootstrap-loader": "^5.0.0",
    "bootstrap-sass": "^3.4.1",
    "chai": "^4.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.2",
    "karma": "^6.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.2.0",
    "node-sass": "^9.0.0",
    "postcss-loader": "^7.3.3",
    "prettier-eslint": "^16.0.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4"
  }
}